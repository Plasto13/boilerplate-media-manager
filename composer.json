{
    "name": "sebastienheyd/boilerplate-media-manager",
    "description": "Media Manager for Laravel Boilerplate",
    "keywords": ["laravel", "boilerplate", "media", "file", "manager", "library", "plugin", "package"],
    "type": "library",
    "require": {
        "intervention/image": "^2.4",
        "sebastienheyd/boilerplate": "5.8.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "squizlabs/php_codesniffer": "^3.0",
        "orchestra/testbench": "^3.8",
        "doctrine/dbal": "^2.9"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Sebastien HEYD",
            "email": "contact@sheyd.fr"
        }
    ],
    "autoload": {
        "psr-4": {
            "Sebastienheyd\\BoilerplateMediaManager\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sebastienheyd\\BoilerplateMediaManager\\BoilerplateMediaManagerServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}